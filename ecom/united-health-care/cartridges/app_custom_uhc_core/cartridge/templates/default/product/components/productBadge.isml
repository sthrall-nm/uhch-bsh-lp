<iscontent type="text/html" charset="UTF-8" compact="true"/>
<isset name="productBadge" value="${product.productBadge}" scope="page"/>
<isif condition="${!empty(productBadge) && productBadge.toString() != 'None'}">
    <div class="product-badge">
      <isprint value="${productBadge}"/>
    </div>
</isif>
